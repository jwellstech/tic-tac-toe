<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacToeController">
   <children>
      <MenuBar fx:id="mbar">
        <menus>
          <Menu fx:id="fileMenu" mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="closeItem" mnemonicParsing="false" text="Close" onAction="#handleClose" />
            </items>
          </Menu>
          <Menu fx:id="helpMenu" mnemonicParsing="false" text="Help">
            <items>
              <MenuItem fx:id="aboutItem" mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <GridPane fx:id="gridPane" prefHeight="380.0" prefWidth="600.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <Pane onMouseEntered="#mouseEntered" onMouseExited="#mouseExited" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</VBox>
